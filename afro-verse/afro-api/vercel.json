{
  "version": 2,
  "ignore": [
    "src/**",
    "dist/**",
    "scripts/**",
    "tests/**"
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "regions": ["iad1"],
  "functions": {
    "api/index.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}

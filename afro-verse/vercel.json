{
  "version": 2,
  "builds": [
    {
      "src": "afro-api/src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/jobs/(.*)",
      "dest": "afro-api/src/server.ts"
    },
    {
      "src": "/api/cron/(.*)",
      "dest": "afro-api/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "afro-api/src/server.ts"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/generation-retry",
      "schedule": "* * * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}







